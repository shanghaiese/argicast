  <servicePage name="HistoryData_ECMWF" trnsTag="All_historicdata_ECMWF_Geotroll" defaultPage="true">
    <pageSettings  allowFavorites="false" allowPrintPage="true" allowEmail="true" allowExportToExcel="true" allowFeedback="false" >
    </pageSettings>
    <nearbyDataPoint name="NearbyDataPoint" defaultDataSource="glb25ECMWF" maxAllowedDistance="500" maxAllowedAltitude="500" resultCount="10" >
    </nearbyDataPoint>
    <agriInfo name="AgriInfo" defaultStartDate="01-01" defaultEndDate="31-12" defaultTemporalAggregation="daily">
      <chart name="daily" trnsTag="vew_fiveinone" width="695" height="300" dataSource="glb25ECMWF" watermark="" >
        <PrimaryAxis trnsTag="precipitation" scaleType="Auto" minValue="-2" maxValue="10" />
        <SecondaryAxis trnsTag="temperaturet" scaleType="Auto" minValue="-20" maxValue="50" />
      </chart>
      <weather name="weather" trnTag="Weather">
        <node id="node0" trnsTag="we_precipitationevapo">
          <series>
            <serie name="PrecipAmount_mm_DailySum" trnsTag="Ge_precipitation" aggregationFunction="sum" isChecked="false" gallery="bar" stacked="false" color="#00CCFF" markertype="none" axisposition="primary" advancedOption="false" labelText="precipitation">
              <advancedOption>
                <allowedAggregationFunction>sum</allowedAggregationFunction>
                <!--allowed aggregation function and first one being default-->
                <!--<allowAccumalation>true</allowAccumalation>-->
                <!--true or false-->
                <minComparisonYear>2001</minComparisonYear>
                <!--With this tag the comparison year will be enabled and the ddl will contain years from minComparisonYear till date-->
                <trend>TrendLTA,TrendMax,TrendMin</trend>
                <!--enum TrendLTA,TrendMax, TrendMin-->
                <!--<altitudeAdjustment>true</altitudeAdjustment>-->
                <!--true or false-->
              </advancedOption>
            </serie>
            <serie name="Evapotranspiration_mm_DailySum" trnsTag="Ge_Evapotranspiration" aggregationFunction="sum" isChecked="false" gallery="line" stacked="false" color="#009966" markertype="none" axisposition="secondary" advancedOption="false" labelText="evapotranspiration"/>
          </series>
        </node>
        <node id="node1" trnsTag="we_temperature">
          <series>
            <serie name="TempAir_C_DaytimeMax" trnsTag="Ge_Temp_Max" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#FF6633" markertype="none" axisposition="primary" advancedOption="false" labelText="temperaturet">
              <advancedOption>
                <allowedAggregationFunction>max,min,sum,avg</allowedAggregationFunction>
                <!--<allowAccumalation>true</allowAccumalation>-->
                <minComparisonYear>2001</minComparisonYear>
                <trend>TrendLTA,TrendMax,TrendMin</trend>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>

            <serie name="TempAir_C_NighttimeMin" trnsTag="Ge_Temp_Min" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#FFCC33" markertype="none" axisposition="primary" advancedOption="false" labelText="temperaturet">
              <advancedOption>
                <allowedAggregationFunction>max,min,sum,avg</allowedAggregationFunction>
                <!--<allowAccumalation>true</allowAccumalation>-->
                <minComparisonYear>2001</minComparisonYear>
                <trend>TrendLTA,TrendMax,TrendMin</trend>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>
            <serie name="TempAir_C_DailyAvg" trnsTag="Ge_Temp_Avg" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#FF9933" markertype="none" axisposition="primary" advancedOption="false" labelText="temperaturet" >
              <advancedOption>
                <allowedAggregationFunction>max,min,sum,avg</allowedAggregationFunction>
                <!--<allowAccumalation>true</allowAccumalation>-->
                <minComparisonYear>2001</minComparisonYear>
                <trend>TrendLTA,TrendMax,TrendMin</trend>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>
            <!--<serie name="TempAir_C_DailyAvgExtremeMin" trnsTag="Ge_Temp_Coldest" aggregationFunction="avg" isChecked="false" gallery="line" stacked="true" color="#FFCC33" markertype="InvertedTriangle" axisposition="primary" advancedOption="false" labelText="temperaturet" >
              <advancedOption>
                <allowAccumalation>true</allowAccumalation>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>
            <serie name="TempAir_C_DailyAvgExtremeMax" trnsTag="Ge_Temp_Warmest" aggregationFunction="avg" isChecked="false" gallery="line" stacked="true" color="#FF6633" markertype="InvertedTriangle" axisposition="primary" advancedOption="false" labelText="temperaturet">
              <advancedOption>
                <allowAccumalation>true</allowAccumalation>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>-->
            <serie name="TempAir_C_DailyAvgLTA" trnsTag="Ge_Temp_LongTerm" aggregationFunction="avg" isChecked="false" gallery="line" stacked="true" color="#FF9933" markertype="InvertedTriangle" axisposition="primary" advancedOption="false" labelText="temperaturet">
              <advancedOption>
                <allowAccumalation>true</allowAccumalation>
                <altitudeAdjustment>true</altitudeAdjustment>
              </advancedOption>
            </serie>
          </series>
        </node>
        <node id="node2" trnsTag="we_humidity">
          <series>
            <serie name="RelHumidity_DailyAvg" trnsTag="Ge_Humidity_Avg" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#0099FF" markertype="none" axisposition="primary" advancedOption="false" labelText="we_relhumidity_percent"/>
            <serie name="RelHumidity_DailyMax" trnsTag="Ge_Humidity_Max" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#0066FF" markertype="none" axisposition="primary" advancedOption="false" labelText="we_relhumidity_percent" />
            <serie name="RelHumidity_DailyMin" trnsTag="Ge_Humidity_Min" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#00CCFF" markertype="none" axisposition="primary" labelText="we_relhumidity_percent"/>
          </series>
        </node>
        <node id="node3" trnsTag="we_radiation">
          <series>
            <serie name="GlobalRadiation_Whm2_DailySum" trnsTag="Ge_GlobalRadiation" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#CCCC33" markertype="none" axisposition="primary" advancedOption="false" labelText="we_radiationseries"/>
            <!--<serie name="GlobalRadiation_Whm2_DailySumLTA" trnsTag="we_lta" aggregationFunction="sum" isChecked="false" gallery="line" stacked="false" color="orange" markertype="triangle" axisposition="primary"/>-->
          </series>
        </node>
        <node id="node4" trnsTag="we_wind">
          <series>
            <serie name="Windspeed_ms_DailyAvg" trnsTag="Ge_Windspeed_Avg" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#C8C8C8" markertype="none" axisposition="primary" advancedOption="false" labelText="we_wind_mps"/>
            <serie name="WindspeedMax_ms_DailyMax" trnsTag="Ge_Windgust" aggregationFunction="avg" isChecked="false" gallery="line" stacked="false" color="#909090" markertype="none" axisposition="primary" labelText="we_wind_mps" />
          </series>
        </node>
      </weather>
    </agriInfo>
  </servicePage>

